{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": 0,
        "linebreak-style": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "no-unused-vars": 1,
        "no-underscore-dangle": 0,
        "react/function-component-definition": 0,
        "react/jsx-no-useless-fragment": 0,
        "react/prop-types": 0,
        "arrow-body-style": 0,
        "react/no-unescaped-entities": 0
    }
}
